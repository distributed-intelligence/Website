{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.@(svg)",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=31536000"
      } ]
    }, {
      "source" : "**/main.min.css",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
      } ]
    } ]
  },
  "short_name" : "Muhammad"
}